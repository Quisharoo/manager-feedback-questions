# Server Configuration
PORT=3000
NODE_ENV=development

# Security
# Secret for signing cookies and HMAC operations (required in production)
COOKIE_SECRET=your-secret-key-here

# Admin Access
# Optional: Enable admin mode and require key for session creation
# Leave empty to allow public session creation without authentication
ADMIN_KEY=

# Vercel KV Storage (optional)
# If using Vercel KV for session storage instead of local files
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=

# Upstash Redis Storage (optional alternative to Vercel KV)
# If using Upstash Redis for session storage
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=
